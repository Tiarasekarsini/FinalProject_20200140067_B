<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.hellocats">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HelloCats"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".HasilDiagnosa"
            android:exported="false" />
        <activity
            android:name=".UpdateJadwal"
            android:exported="false" />
        <activity
            android:name=".TambahJadwal"
            android:exported="false" />
        <activity
            android:name=".UpdateKucing"
            android:exported="false" />
        <activity
            android:name=".TambahKucing"
            android:exported="false"
            android:parentActivityName=".MyCats_Utama" />
        <activity
            android:name=".Riwayat_Diagnosa"
            android:label="Riwayat Diagnosa"
            android:exported="false"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Diagnosa_Utama"
            android:label="Diagnosa"
            android:exported="false"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Jadwal_Utama"
            android:label="Jadwal Vaksinasi"
            android:exported="false"
            android:parentActivityName=".Home" />
        <activity
            android:name=".MyCats_Utama"
            android:label="MyCats"
            android:exported="true"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Home"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>